{
  "vhosts": [{ "name": "/" }],
  "users": [
    {
      "name": "admin",
      "password_hash": "$ADMIN_PASSWORD_HASH",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "administrator"
    },
    {
      "name": "app_user",
      "password_hash": "$APP_USER_PASSWORD_HASH",
      "hashing_algorithm": "rabbit_password_hashing_sha256"
    }
  ],
  "permissions": [
    {
      "user": "admin", "vhost": "/",
      "configure": ".*", "write": ".*", "read": ".*"
    },
    {
      "user": "app_user", "vhost": "/",
      "configure": "", "write": ".*", "read": ".*"  }
  ],
  "queues": [
    {
      "name": "queue_a",
      "vhost": "/",
      "durable": true
    },
    {
      "name": "queue_b",
      "vhost": "/",
      "durable": true
    },
    {
      "name": "queue_c",
      "vhost": "/",
      "durable": true
    }
  ]
}
